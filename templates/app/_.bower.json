{
  "name": "<%= config['bower.id'] %>",
  "version": "0.0.1",
  "description": "<%= config['project.description'] %>",
  "authors": [
    "<%= config['author.name'] %> <<%= config['author.email'] %>> (<%= config['author.url'] %>)"
  ],
  "license": "<%= config['license.name'] %>",
  "homepage": "<%= config['homepage.url'] %>",
  "ignore": [
    "bin",
    "docs",
    "grunt",
    ".bowerrc",
    ".editorconfig",
    ".gitignore",
    ".jsbeautifyrc",
    ".jshintrc",
    ".travis.yml",
    "Gruntfile.js",
    "TODO.md",
    "bootstrap.sh",
    "package.json",
    "travis.sh"
  ],
  "repository": {
    "type": "git",
    "url": "<%= config['repo.url'] %>"
  },
  "dependencies": {
    "angular": "~1.3.8"
  },
  "devDependencies": {
    "angular-mocks": "~1.3.8"
  }
}
